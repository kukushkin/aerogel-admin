<% page_title t.aerogel.admin.views.users.create.title %>

<% form @admin_user_new_form, action: "/admin/users/new" do %>
<%=     field :full_name %>
<%=     field :email %>
<%=     field :roles, as: :multiselect, from: Role, values: :slug, labels: :name %>


<%=     button :cancel, url: '/admin/users/' %>
<%=     button :create %>
<% end %>
